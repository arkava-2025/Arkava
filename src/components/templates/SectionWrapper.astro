---
import Faq from '@/components/organisms/Faq';
import Alianzas from '@/components/organisms/Alianzas.jsx';
import NoComponentFile from '@/components/NoComponentFile.astro';

const { __typename, ...rest } = Astro.props;
const typeKey = (
  typeof __typename === 'string' ? __typename : `${__typename ?? ''}`
).toLowerCase();
---

{typeKey === 'alianzas' && <Alianzas {...rest} client:load />}
{typeKey === 'faq' && <Faq {...rest} client:load />}

{typeKey !== 'alianzas' && typeKey !== 'faq' && <NoComponentFile {...rest} />}
